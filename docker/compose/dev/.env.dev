# Dev compose resolves channel dynamically; just keep these in sync with ops/ci/vars.env
REGISTRY=ghcr.io
REGISTRY_NS=cdaprod
SERVICE_EDGE=thatdam-run
CHANNEL=dev
EDGE_IMG_CHANNEL=${REGISTRY}/${REGISTRY_NS}/${SERVICE_EDGE}:${CHANNEL}

# (Optional) Example overrides to exercise discovery & flags in dev
FEATURE_FLAGS=dev,debug
MINIO_ENDPOINT=http://minio:9000
WEAVIATE_ENDPOINT=http://weaviate:8080
TAILSCALE_AUTHKEY=
