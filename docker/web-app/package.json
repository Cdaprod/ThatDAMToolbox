{
  "name": "thatdamtoolbox-web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo -p 3000",
    "dev:warm": "concurrently -k -n next,warmup -c auto \"next dev --turbo -p 3000\" \"tsx scripts/dev-warmup.ts\"",
    "build": "next build",
    "start": "NODE_ENV=production node start.js start",
    "export": "next export -o build",
    "build:export": "rm -rf build && next build && next export -o build",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "docker:build": "docker build -t thatdamtoolbox-web-app:dev .",
    "docker:run": "docker run --rm -p 3000:3000 thatdamtoolbox-web-app:dev",
    "docker:sh": "docker run -it --rm thatdamtoolbox-web-app:dev sh",
    "test": "rm -rf .tmp-test && tsc -p tsconfig.test.json && NODE_ENV=test node --require ./src/test/test-mocks.js --test $(find .tmp-test -name '*.test.js')"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@react-three/drei": "^9.56.7",
    "@react-three/fiber": "^8.13.5",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@thatdamtoolbox/stream-registry": "file:../packages/stream-registry",
    "amqplib": "^0.10.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.11",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "gl-react": "^5.2.0",
    "gl-react-dom": "^5.2.1",
    "lucide-react": "^0.357.0",
    "mitt": "^3.0.1",
    "next": "15.5.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^4.10.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.3.6",
    "three": "^0.179.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/three": "^0.179.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "15.5.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "react-devtools-core": "^6.1.5",
    "typescript": "^5.2.2",
    "concurrently": "^9.0.0",
    "tsx": "^4.16.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
